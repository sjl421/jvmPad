
public class LinkedList {
	Node start;
	void insertFront(int element){
		Node newnode=new Node(element);
		Node temp=null;
		if(start==null) start=newnode;
		else {
			temp=start;
			start=newnode;
			start.next=temp;
		}
	}
	void insertRear(int element){
		Node ptr=null;
		Node newnode=new Node(element);
		while(ptr!=null) {
			ptr=ptr.next;
		}
		ptr=newnode;
	}
}
